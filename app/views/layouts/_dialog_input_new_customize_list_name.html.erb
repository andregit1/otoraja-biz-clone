<div id="dialogInputNewCustomizeListName" class="modal" tabindex="-1" role="dialog" aria-labelledby="inputListName" aria-hidden="true" data-backdrop="true" data-keyboard="false">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
      <div class="modal-header management-header">
        <h5 class="modal-title"><%= t('customize_list.new_list_name.title') %></h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <div class="form-group">
          <input type="text" id="new_customize_list_name" class="form-control">
        </div>
      </div>
      <div class="modal-footer">
        <div class="row">
          <div class="col">
            <button type="button" class="btn btn-base -basic -normal" data-dismiss="modal">Cancel</button>
          </div>
          <div class="col">
            <button type="button" class="btn btn-base -basic -primary" id="btn_create_customize_list">OK</button>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
